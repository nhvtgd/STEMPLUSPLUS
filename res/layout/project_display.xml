    android:layout_height="match_parent"
    android:orientation="vertical" >

    <ImageView
        android:id="@+id/project_avatar_all_project_display"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" 
        android:background="@drawable/yellow_border_image"
        android:src="@drawable/image_test" >
    </ImageView>


    <TextView
        android:id="@+id/project_description_all_project_display"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/project_description_allProjecDisplay" />

</RelativeLayout>
